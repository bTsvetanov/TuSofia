print("Въведете страните на триъгълника:")  
a = int(input("a = "))
b = int(input("b = "))
c = int(input("c = "))

if a + b <= c or a + c <= b or b + c <= a:
    print("не е триъгълник")
else:
    if a == b == c:
        print("равностранен")
    elif a == b or b == c or a == c:
        print("равнобедрен")
    else:
        print("разностранен")
